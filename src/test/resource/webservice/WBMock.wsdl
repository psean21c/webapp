<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<wsdl:definitions name="FXRateService" 
xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" 
xmlns:tns="http://www.example.org/WBMock/" 
xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" 
xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
targetNamespace="http://www.example.org/WBMock/" 
xmlns:wbfxr="http://emf.cibc.com/emb/wbfxr">

  <wsdl:types>
    <xsd:schema targetNamespace="http://www.emf.cibc.com/emb/wbfxr">
			<xsd:import namespace="http://emf.cibc.com/emb/wbfxr/"
				schemaLocation="../xsd/wbfxr.xsd">
			</xsd:import>
    </xsd:schema>
  </wsdl:types>
  
  <wsdl:message name="GetRateInput">
    <wsdl:part element="tns:GetRateInput" name="getRateRequest"/>
  </wsdl:message>
  <wsdl:message name="GetRateOutput">
    <wsdl:part element="tns:GetRateOutput" name="getRateResponse"/>
  </wsdl:message>
  
  <wsdl:portType name="WBMock">
    <wsdl:operation name="GetRateInput">
      <wsdl:input message="tns:GetRateInput"/>
      <wsdl:output message="tns:GetRateOutput"/>
    </wsdl:operation>
  </wsdl:portType>
  
  <wsdl:binding name="WBMockSOAP" type="tns:WBMock">
    <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
    <wsdl:operation name="GetRateInput">
      <soap:operation soapAction="http://www.example.org/WBMock/GetRateInput"/>
      <wsdl:input>
        <soap:body use="literal"/>
      </wsdl:input>
      <wsdl:output>
        <soap:body use="literal"/>
      </wsdl:output>
    </wsdl:operation>
  </wsdl:binding>
  
  <wsdl:service name="FXRateService">
    <wsdl:port binding="tns:FXRateServiceImplPortBinding" name="FXRateServiceImplPort">
      <soap:address location="http://localhost:8092"/>
    </wsdl:port>
  </wsdl:service>
</wsdl:definitions>
